-- ripple

function rppl(x,y,r,s)
	c={}
	c.x=x
	c.y=y
	c.r=r
	c.s=s
	
	function c.draw(o)
		oval(o.x-o.r*2+1,o.y-o.r+6,
								o.x+o.r*2+1,o.y+o.r+6,7)
		if o.r > s*0.7 then
		oval(o.x-o.r*2-0.8+1,o.y-o.r+6,
							o.x+o.r*2+0.8+1,o.y+o.r+6,12)
		end
		o.r+=min(0.5,(13-o.r)/13)
	end
	return c
end
