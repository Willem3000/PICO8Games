-- fish

function fish(name,sprt,str,ctchmtr)
	local c = {}
	c.x=64
	c.y=-20
	c.v=0
	c.name=name
	c.sprt=sprt
	c.str=str
	c.ctchmtr=ctchmtr
	c.caught=false
	
	function c.draw(o)
		if c.v<0 then
			silhouette()
		end
		spr(o.sprt,o.x-12,o.y,3,2)
		pal()
	end
	return c
end
